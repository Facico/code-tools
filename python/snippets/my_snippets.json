	"ex-ipdbm":{
		"prefix": "ex-ipdbm",
		"body": [
			"import sys",
			"import pdb",
			"",
			"class ForkedPdb(pdb.Pdb):",
    		"	def interaction(self, *args, **kwargs):",
        	"		_stdin = sys.stdin",
        	"		try:",
            "			sys.stdin = open('/dev/stdin')",
            "			pdb.Pdb.interaction(self, *args, **kwargs)",
        	"		finally:",
            "			sys.stdin = _stdin",
			"",
			"ForkedPdb().set_trace()",
		]
	},
